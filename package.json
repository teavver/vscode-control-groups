{
  "name": "vscode-control-groups",
  "displayName": "vscode-control-groups",
  "description": "sc2 control groups for vscodevim extension",
  "author": {
    "name": "teavver"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionDependencies": [
    "vscodevim.vim"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "keybindings": [
        {
              "command": "sc2.createControlGroup",
              "key": "ctrl+1",
              "when": "editorTextFocus",
              "args": { "id": 1 }
          },
          {
              "command": "sc2.createControlGroup",
              "key": "ctrl+2",
              "when": "editorTextFocus",
              "args": { "id": 2 }
          },
          {
              "command": "sc2.createControlGroup",
              "key": "ctrl+3",
              "when": "editorTextFocus",
              "args": { "id": 3 }
          },
          {
              "command": "sc2.createControlGroup",
              "key": "ctrl+4",
              "when": "editorTextFocus",
              "args": { "id": 4 }
          },
          {
              "command": "sc2.createControlGroup",
              "key": "ctrl+5",
              "when": "editorTextFocus",
              "args": { "id": 5 }
          }
      ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
